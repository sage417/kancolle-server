<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kancolle.server.dao.member.MemberUseItemDao">

	<cache type="org.mybatis.caches.ehcache.EhcacheCache" />

	<resultMap type="UseItem" id="UseItemRM" autoMapping="true">
		<id property="useitemId" column="ID" />
		<result property="description" column="DESCRIPTION" typeHandler="JsonArrayHanlder"/>
	</resultMap>
	
	<select id="selectUseItemByCond" resultMap="UseItemRM" >
		SELECT * FROM t_useitem
		<where>
			<if test="_parameter != null">ID = #{useitem_id}</if>
		</where>
	</select>
</mapper>